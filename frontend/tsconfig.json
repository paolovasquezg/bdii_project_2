
{

  "compilerOptions": {

    "target": "ES2020",

    "lib": ["DOM", "ESNext"],

    "jsx": "react-jsx",

    "module": "ESNext",

    "moduleResolution": "Node",

    "strict": true,

    "esModuleInterop": true,

    "skipLibCheck": true,

    "forceConsistentCasingInFileNames": true,

    "isolatedModules": true,

    "resolveJsonModule": true,

    "allowJs": true,

    "noEmit": true,

    "baseUrl": ".",

    "paths": {}

  },

  "include": ["src"]

}

